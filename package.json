{
  "name": "react-express-boilerplate",
  "version": "1.0.0",
  "description": "a very flexible boilerplate with react, redux, express, sass, eslint, stylelint and mocha",
  "main": "index.js",
  "scripts": {
    "dev-windows": "node_modules/.bin/npm-run-all --parallel dev-wepback-windows dev-nodemon-windows",
    "dev-wepback-windows": "node webpack-dev-server.js",
    "dev-nodemon-windows": "set PORT=8001&&set NODE_ENV=development&&nodemon start.js -e js,jsx",
    "build": "webpack --config webpack-production.config.js",
    "start": "NODE_ENV='production' node entry.js",
    "lint": "eslint ./ --ext .js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddanilets/react-express-boilerplate.git"
  },
  "author": "ddanilets",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ddanilets/react-express-boilerplate/issues"
  },
  "homepage": "https://github.com/ddanilets/react-express-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-thunk": "^2.1.0",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.0.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2"
  }
}
